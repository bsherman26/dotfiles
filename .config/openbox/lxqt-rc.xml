    * Volume Management
    ********************************** -->

    <!-- Volume Raise -->
    <keybind key="XF86AudioRaiseVolume">
        <action name="Execute">
            <command>amixer -q -D pulse sset Master 3%+ unmute</command>
        </action>
    </keybind>

    <!-- Volume Lower -->
    <keybind key="XF86AudioLowerVolume">
        <action name="Execute">
            <command>amixer -q -D pulse sset Master 3%- unmute</command>
        </action>
    </keybind>

    <!-- Volume Mute -->
    <keybind key="XF86AudioMute">
        <action name="Execute">
            <command>amixer -q -D pulse sset Master toggle</command>
        </action>
    </keybind>


