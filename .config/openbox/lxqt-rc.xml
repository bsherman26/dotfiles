    <!--********************************************************
    * Window * Tiling
    **********************
    * SUMMARY:
    *
    *   (Super + " ")        => Window > Resize > Half-Screen
    *
    *     "H"      => Left
    *     "L"      => Right
    *     "K"      => Top
    *     "J"      => Bottom
    *
    *   (Super + Alt + " ")  => Window > Resize > Corner-of-Screen
    *
    *     "H"      => Top-Left
    *     "L"      => Top-Right
    *     "K"      => Bottom-Right
    *     "J"      => Bottom-Left
    *
    *   (Super + Ctrl + " ") => Window > Move   > To-Monitor
    *
    *     "H"      => Monitor-Next
    *     "L"      => Monitor-Previous
    *
    ********************************************************-->

        <!-- Half-Left Screen -->
    <keybind key="W-H">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>0</y>
            <height>100%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Half-Right Screen -->
    <keybind key="W-L">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>-0</x>
            <y>0</y>
            <height>100%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Half-Upper Screen -->
    <keybind key="W-K">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>0</y>
            <width>100%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Half-Lower Screen -->
    <keybind key="W-J">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>-0</y>
            <width>100%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Top-Left-Corner of Screen -->
    <keybind key="W-A-H">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>0</y>
            <height>50%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Top-Right-Corner of Screen -->
    <keybind key="W-A-L">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>-0</x>
            <y>0</y>
            <height>50%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Bottom-Right-Corner of Screen -->
    <keybind key="W-A-K">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>-0</x>
            <y>-0</y>
            <width>50%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Bottom-Left-Corner of Screen -->
    <keybind key="W-A-J">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>-0</y>
            <width>50%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Half-Left Screen -->
    <keybind key="W-Left">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>0</y>
            <height>97%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Half-Right Screen -->
    <keybind key="W-Right">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>-0</x>
            <y>0</y>
            <height>97%</height>
            <width>50%</width>
        </action>
    </keybind>

    <!-- Half-Upper Screen -->
    <keybind key="W-Up">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>0</y>
            <width>100%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Half-Lower Screen -->
    <keybind key="W-Down">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
            <x>0</x>
            <y>-0</y>
            <width>100%</width>
            <height>50%</height>
        </action>
    </keybind>

    <!-- Monitor-Next -->
    <keybind key="W-C-l">
        <action name="MoveResizeTo">
            <monitor>next</monitor>
        </action>
    </keybind>

    <!-- Monitor-Previous -->
    <keybind key="W-C-h">
        <action name="MoveResizeTo">
            <monitor>prev</monitor>
        </action>
    </keybind>


    <!--********************************************************
    * App * Launch / etc.
    **********************
    * SUMMARY:
    *
    *   (Ctrl + Alt + " ")
    *
    *     "T"      => Terminator (Default)   (terminator)
    *     "P"      => Screen Shot            (lximage-qt -\-screenshot)
    *     "Delete" => Task Manager           (qps)
    *
    *   (Ctrl + Alt + Shift + " ")
    *
    *     "T" => Terminal                    (terminator)
    *     "T" => Terminator Config Launcher  (terminator -s)
    *     "V" => Gvim                        (gvim)
    *     "I" => Internet Brower             (google-chrome-stable)
    *     "F" => File Manager                (pcmanfm-qt)
    *     "C" => Calculator                  (kcalc)
    *     "U" => App Runner                  (lxqt-runner)
    *     "S" => Screen > Off                (xscreensaver-command -activate) (With Blank Screensaver)
    *                                            => True Off => (xset dpms force off)
    *     "P" => Screen > Lock               (xscreensaver-command -lock)
    *                                            => LXQT     => (lxqt-leave -\-lockscreen)
    *     "O" => Shutdown Menu               (lxqt-leave)
    *
    ********************************************************-->

    <!-- Terminal - Launch (Ctrl + Alt + T) -->
    <keybind key="C-A-T">
        <action name="Execute">
          <command>/usr/bin/terminator</command>
          <!--<command>/usr/bin/qterminal</command>-->
        </action>
    </keybind>

    <!-- Terminator-Config-Launcher (Ctrl + Alt + Shift + T) -->
    <keybind key="C-A-S-T">
        <action name="Execute">
            <command>/usr/bin/terminator -s</command>
        </action>
    </keybind>

    <!-- Gvim (Ctrl + Alt + V) -->
    <keybind key="C-S-A-V">
        <action name="Execute">
            <command>/usr/bin/gvim</command>
        </action>
    </keybind>

    <!-- Internet-Browser (Ctrl + Alt + Shift + I) -->
    <keybind key="C-A-S-I">
        <action name="Execute">
            <command>/usr/bin/google-chrome-stable</command>
        </action>
    </keybind>

    <!-- Filemanager (Ctrl + Alt + Shift + F) -->
    <keybind key="C-A-S-F">
        <action name="Execute">
            <startupnotify>
                <enabled>true</enabled>
                <name>PCManFM</name>
            </startupnotify>
            <command>/usr/bin/pcmanfm-qt --profile=lxqt</command>
        </action>
    </keybind>

    <!-- Take a screenshot of the current window with scrot when Alt+Print are pressed -->
    <keybind key="C-A-P">
    <!-- <keybind key="A-Print"> -->
        <action name="Execute">
            <command>/usr/bin/lximage-qt --screenshot</command>
            <!--<command>/usr/bin/screengrab</command>-->
        </action>
    </keybind>

    <!-- Calculator (Ctrl + Alt + Shift + C) -->
    <keybind key="C-A-S-C">
        <action name="Execute">
            <command>/usr/bin/kcalc</command>
        </action>
    </keybind>

    <!-- App Runner (Ctrl + Alt + Shift + U) -->
    <keybind key="C-A-S-U">
        <action name="Execute">
            <command>/usr/bin/lxqt-runner</command>
        </action>
    </keybind>

    <!-- Task Manager (Ctrl + Alt + Del) -->
    <keybind key="C-A-Delete">
        <action name="Execute">
            <command>/usr/bin/qps</command>
        </action>
    </keybind>

    <!-- Screen-Off (Ctrl + Alt + Shift + P) -->
    <keybind key="C-A-S-P">
    <!--<keybind key="C-A-S-S">-->
        <action name="Execute">
            <command>xscreensaver-command -activate</command>
              <!--<command>xset dpms force off</command>-->
        </action>
    </keybind>

    <!-- Screen-Lock (Ctrl + Alt + Shift + Print) -->
    <keybind key="C-A-Print">
    <!--<keybind key="C-A-S-P">-->
        <action name="Execute">
            <command>xscreensaver-command -lock</command>
            <!--<command>lxqt-leave -/-lockscreen</command>-->
        </action>
    </keybind>

    <!-- Shutdown-Menu (Ctrl + Alt + Shift + O) -->
    <keybind key="C-A-S-O">
        <action name="Execute">
            <command>lxqt-leave</command>
        <!--<command>lxqt-leave -\-logout</command>-->
        <!--<command>lxqt-leave -\-lockscreen</command>-->
        <!--<command>lxqt-leave -\-suspend</command>-->
        <!--<command>lxqt-leave -\-hibernate</command>-->
        <!--<command>lxqt-leave -\-shutdown</command>-->
        <!--<command>lxqt-leave -\-reboot</command>-->
        </action>
    </keybind>


